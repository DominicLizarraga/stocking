<%= form_with url: "/", method: :post do |form| %>
  <%= form.label :query, "Search for:" %>
  <%= form.text_field :query %>
  <%= form.submit "Search" %>
<% end %>

<% if @stock %>
  <%= @stock.company_name %>
<% elsif @nothing %>
  <%= @nothing %>
<% end %>


<% if @error %>
  <%= @error %>
<% end %>
